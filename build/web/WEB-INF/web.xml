<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ThemLoaiChiTieu</servlet-name>
        <servlet-class>Controler.LoaiChiTieu.ThemLoaiChiTieu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TuyBien</servlet-name>
        <servlet-class>Controler.LoaiChiTieu.TuyBien</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TuyBien_Quydinh</servlet-name>
        <servlet-class>Controler.QuyDinh.TuyBien_Quydinh</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThemChiTieu</servlet-name>
        <servlet-class>Controler.ChiTieu.ThemChiTieu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TuyBien_ChiTieu</servlet-name>
        <servlet-class>Controler.ChiTieu.TuyBien_ChiTieu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TuyBien_ThongBao</servlet-name>
        <servlet-class>Controler.ThongBao.TuyBien_ThongBao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ThemLoaiChiTieu</servlet-name>
        <url-pattern>/ThemLoaiChiTieu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TuyBien</servlet-name>
        <url-pattern>/TuyBien</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TuyBien_Quydinh</servlet-name>
        <url-pattern>/TuyBien_Quydinh</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThemChiTieu</servlet-name>
        <url-pattern>/ThemChiTieu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TuyBien_ChiTieu</servlet-name>
        <url-pattern>/TuyBien_ChiTieu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TuyBien_ThongBao</servlet-name>
        <url-pattern>/TuyBien_ThongBao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
